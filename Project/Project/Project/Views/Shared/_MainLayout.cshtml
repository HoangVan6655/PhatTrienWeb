<body>
    <!--Wrapper-->
    <div id="wrapper">
        <!--Page-->
        <div class="page shadow">
            <!--Header-->
            <div id="header">
                <div class="primary-section">
                    <div class="logo"> <a href="http://www.webgranth.com"><img src="~/images/logo.png" alt="logo" title="webgranth" /></a></div>
                    <div class="header-aside">
                        <span>VShop</span>
                        <ul>
                            <partial name="_LoginPartial" />
