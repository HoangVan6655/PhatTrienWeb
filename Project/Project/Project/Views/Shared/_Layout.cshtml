    <script src="~/js/jquery-1.9.1.min.js" type="text/javascript"></script>
    <script src="~/js/slides.min.jquery.js" type="text/javascript"></script>
    <script type="text/javascript">$(function () {
            $('.slides').slides({
                preload: true,
                generatePagination: true,
                play: 3000
            });
        });</script>

